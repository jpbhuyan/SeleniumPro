<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="none" verbose="10">
<groups>
  <run>
      <include name="Smoke"></include>
      <include name="Sanity"></include>
      <include name="Regression"></include>
      <include name="Common"></include>
  </run>
</groups>

  <test name="Test">
    <classes>
      <class name="tests.EditLeads"/>
      <class name="tests.MergeLeads"/>
      <class name="tests.DuplicateLead"/>
      <class name="tests.CreateLead"/>
      <class name="tests.DeleteLead"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
